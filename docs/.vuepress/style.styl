#my-style {
}

.content
  pre, pre[class*="language-"]
    code
      color #212121
      font-weight normal

    span[class*="token"]
      color #212121

    span[class="token class-name"]
      color #7c00bc

    span[class="token keyword"]
      color #d72b28
      font-weight bold

    span[class="token string"]
      color #d72b28

    span[class="token function"]
      color #0066bc

    span[class="token comment"]
      color #7e7e7e


.navbar
  background-color #fafafa
  box-shadow: 0px 1px 1px #eeeeee

  .logo
    margin-right 0

  .links
    background-color #fafafa

  .search-box input
    border-radius 0


