#my-style {
}

.content
  pre, pre[class*="language-"]
    code
      color #212121
      font-weight normal

    span[class*="token"]
      color #212121

    span[class="token class-name"]
      color #9c27b0

    span[class="token keyword"]
      color #c62828
      font-weight bold

    span[class="token string"]
      color #d84315

    span[class="token function"]
      color #0d47a1

    span[class="token comment"]
      color #9e9e9e

    span[class*="token macro"]
      color #4caf50

    span[class*="token directive"]
      color #4caf50

.home .hero img
  max-height 200px

.sidebar-heading
  font-size 1.0rem

.sidebar-link
  font-size 0.9rem

.navbar
  background-color #fafafa
  box-shadow: 0px 1px 1px #eeeeee

  .logo
    margin-right 0

  .links
    background-color #fafafa

  .search-box input
    border-radius 0


